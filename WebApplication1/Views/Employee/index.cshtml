
     @model IEnumerable<Websais.Models.EmployeeView>
@{
    ViewBag.Title = "Сотрудники";
}
<table>
    <thead>
        <tr>
            <th></th>
            <th>id</th>
            <th>Фамилея</th>
            <th>Имя</th>
            <th>Отчества</th>
            <th>Возраст</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                
                <td>@employee.FirstName</td>

                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
                



            </tr>
        }
    </tbody>
</table>


<th />
